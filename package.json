{
  "name": "@hit/feature-pack-notepad",
  "version": "1.0.53",
  "description": "Notepad feature pack with CRUD, list view, sharing, and per-user/global scope. Uses @hit/ui-kit for shell-agnostic UI.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/schema/notepad.d.ts",
      "import": "./dist/schema/notepad.js",
      "require": "./dist/schema/notepad.js",
      "default": "./dist/schema/notepad.js"
    },
    "./server/api/*": {
      "types": "./dist/server/api/*.d.ts",
      "import": "./dist/server/api/*.js",
      "require": "./dist/server/api/*.js",
      "default": "./dist/server/api/*.js"
    }
  },
  "files": [
    "dist",
    "feature-pack.yaml"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {},
  "peerDependencies": {
    "@hit/acl-utils": "github:c0x65o/hit-acl-utils#cfa9481d9da5b1a73642efc56fa3ca76cb6b7cac",
    "@hit/feature-pack-auth-core": "github:c0x65o/hit-feature-pack-auth-core#90d875b579c7a602440b68a6dc8cf1ce35e776aa",
    "@hit/sdk": "git+https://github.com/c0x65o/hit-sdk-typescript.git#8b77bff5b14ca3541a4e93830cbb913ff58331d6",
    "@hit/ui-kit": "github:c0x65o/hit-ui-kit#4237e657aed7de52c31642dd5c27d0cc4bcbc9cf",
    "drizzle-orm": "^0.38.0",
    "lucide-react": "^0.460.0",
    "next": "^15.5.9",
    "react": "^19.2.3",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@hit/acl-utils": "github:c0x65o/hit-acl-utils#cfa9481d9da5b1a73642efc56fa3ca76cb6b7cac",
    "@hit/feature-pack-auth-core": "github:c0x65o/hit-feature-pack-auth-core#90d875b579c7a602440b68a6dc8cf1ce35e776aa",
    "@hit/sdk": "git+https://github.com/c0x65o/hit-sdk-typescript.git#8b77bff5b14ca3541a4e93830cbb913ff58331d6",
    "@hit/ui-kit": "github:c0x65o/hit-ui-kit#4237e657aed7de52c31642dd5c27d0cc4bcbc9cf",
    "@types/node": "^20",
    "@types/react": "^19.0.0",
    "drizzle-orm": "^0.38.0",
    "lucide-react": "^0.460.0",
    "next": "^15.5.9",
    "typescript": "^5",
    "zod": "^4.0.0"
  },
  "keywords": [
    "hit",
    "feature-pack",
    "notepad",
    "notes",
    "crud",
    "ui-kit"
  ],
  "license": "MIT"
}
